<?xml version="1.0" encoding="utf-8"?>
<full-backup-content>
    <!-- SharedPreferences 백업 (device_id 등) -->
    <include domain="sharedpref" path="FlutterSecureStorage" />
    
    <!-- 모든 SharedPreferences 백업 -->
    <include domain="sharedpref" path="." />
    
    <!-- 기타 불필요한 데이터 제외 -->
    <exclude domain="file" path="cache/" />
    <exclude domain="database" path="cache.db" />
</full-backup-content>